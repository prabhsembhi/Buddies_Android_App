<?xml version="1.0" encoding="utf-8"?><androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"    xmlns:app="http://schemas.android.com/apk/res-auto"    xmlns:tools="http://schemas.android.com/tools"    android:layout_width="match_parent"    android:layout_height="match_parent"    tools:context=".groupActivity">    <TextView        android:id="@+id/textView10"        android:layout_width="293dp"        android:layout_height="58dp"        android:layout_marginStart="8dp"        android:layout_marginLeft="8dp"        android:layout_marginTop="96dp"        android:layout_marginEnd="8dp"        android:layout_marginRight="8dp"        android:text="Welcom to BUDDEIS!"        android:textColor="#1F308F"        android:textSize="30sp"        android:textStyle="bold"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toTopOf="@+id/container" />    <Button        android:id="@+id/btnMap"        android:layout_width="301dp"        android:layout_height="109dp"        android:layout_marginStart="8dp"        android:layout_marginLeft="8dp"        android:layout_marginEnd="8dp"        android:layout_marginRight="8dp"        android:layout_marginBottom="100dp"        android:background="@drawable/textarea2"        android:text="Explore people in the map"        android:textColor="#FFFFFF"        android:textStyle="bold"        app:layout_constraintBottom_toTopOf="@+id/navigationView"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintHorizontal_bias="0.542"        app:layout_constraintStart_toStartOf="parent" />    <ImageView        android:id="@+id/imageView5"        android:layout_width="281dp"        android:layout_height="204dp"        android:layout_marginStart="8dp"        android:layout_marginLeft="8dp"        android:layout_marginTop="8dp"        android:layout_marginEnd="8dp"        android:layout_marginRight="8dp"        app:layout_constraintBottom_toTopOf="@+id/btnMap"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintHorizontal_bias="0.482"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toBottomOf="@+id/textView10"        app:srcCompat="@drawable/image" />    <FrameLayout        android:id="@+id/container"        android:layout_width="match_parent"        android:layout_height="match_parent"        app:layout_constraintBottom_toBottomOf="parent"        app:layout_constraintHorizontal_bias="0.0"        app:layout_constraintLeft_toLeftOf="parent"        app:layout_constraintRight_toRightOf="parent">    </FrameLayout><com.google.android.material.bottomnavigation.BottomNavigationViewandroid:id="@+id/navigationView"android:layout_width="0dp"android:layout_height="wrap_content"android:background="@color/navigation"app:itemBackground="@color/navigation"app:itemTextColor="@color/navigation2"app:layout_constraintBottom_toBottomOf="parent"app:layout_constraintLeft_toLeftOf="parent"app:layout_constraintRight_toRightOf="parent"app:menu="@menu/navigation" />    </androidx.constraintlayout.widget.ConstraintLayout>